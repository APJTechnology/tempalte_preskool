{"ast":null,"code":"export default {\n  data: function data() {\n    return {\n      title: 'Rating',\n      items: [{\n        text: 'Dashboard',\n        href: '/index'\n      }, {\n        text: 'Components',\n        active: true\n      }]\n    };\n  }\n};","map":{"version":3,"mappings":"AAwHA,eAAe;EACbA,IAAI,kBAAG;IACL,OAAO;MACLC,KAAK,EAAE,QAAQ;MACfC,KAAK,EAAE,CACL;QACEC,IAAI,EAAE,WAAW;QACjBC,IAAI,EAAE;MACR,CAAC,EACD;QACED,IAAI,EAAE,YAAY;QAClBE,MAAM,EAAE;MACV,CAAC;IAEL,CAAC;EACH;AACF,CAAC","names":["data","title","items","text","href","active"],"sourceRoot":"","sources":["/home/jesus-adson/projects/erp-front/src/views/pages/uiinterface/elements/rating.vue"],"sourcesContent":["<template>\n  <!-- Main Wrapper -->\n  <div class=\"main-wrapper\">\n    <layout-header></layout-header>\n    <layout-sidebar></layout-sidebar>\n\n    <!-- Page Wrapper -->\n    <div class=\"page-wrapper\">\n      <div class=\"content container-fluid\">\n        <componentheader :title=\"title\" :items=\"items\" />\n        <div class=\"row\">\n          <!-- Rating -->\n          <div class=\"col-md-6\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <h5 class=\"card-title\">Default Rating</h5>\n              </div>\n              <div class=\"card-body\">\n                <p>This is the most basic example of ratings.</p>\n                <star-rating v-bind:star-size=\"20\" v-bind:show-rating=\"false\" />\n              </div>\n            </div>\n          </div>\n          <!-- /Rating -->\n\n          <!-- Rating -->\n          <div class=\"col-md-6\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <h5 class=\"card-title\">Score</h5>\n              </div>\n              <div class=\"card-body\">\n                <p>Stars with a saved rating.</p>\n                <star-rating\n                  v-bind:star-size=\"20\"\n                  v-bind:rating=\"3\"\n                  v-bind:show-rating=\"false\"\n                />\n              </div>\n            </div>\n          </div>\n          <!-- /Rating -->\n\n          <!-- Rating -->\n          <div class=\"col-md-6\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <h5 class=\"card-title\">Max Number</h5>\n              </div>\n              <div class=\"card-body\">\n                <p>Change the max numbers of stars</p>\n                <star-rating\n                  v-bind:star-size=\"20\"\n                  v-bind:max-rating=\"10\"\n                  v-bind:show-rating=\"false\"\n                />\n              </div>\n            </div>\n          </div>\n          <!-- /Rating -->\n\n          <!-- Rating -->\n          <div class=\"col-md-6\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <h5 class=\"card-title\">Read Only</h5>\n              </div>\n              <div class=\"card-body\">\n                <p>Prevent users from voting</p>\n                <star-rating\n                  v-bind:star-size=\"20\"\n                  v-bind:read-only=\"true\"\n                  v-bind:rating=\"3\"\n                  v-bind:show-rating=\"false\"\n                />\n              </div>\n            </div>\n          </div>\n          <!-- /Rating -->\n\n          <!-- Rating -->\n          <div class=\"col-md-6\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <h5 class=\"card-title\">Half Rating</h5>\n              </div>\n              <div class=\"card-body\">\n                <p>You can represent a float score as a half star icon.</p>\n                <star-rating\n                  v-bind:star-size=\"20\"\n                  v-bind:increment=\"0.5\"\n                  v-bind:rating=\"3.5\"\n                  v-bind:show-rating=\"false\"\n                />\n              </div>\n            </div>\n          </div>\n          <!-- /Rating -->\n\n          <!-- Rating -->\n          <div class=\"col-md-6\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <h5 class=\"card-title\">Custom Icon</h5>\n              </div>\n              <div class=\"card-body\">\n                <p>Use any icon you want.</p>\n                <star-rating v-bind:star-size=\"20\" v-bind:show-rating=\"false\" />\n              </div>\n            </div>\n          </div>\n          <!-- /Rating -->\n        </div>\n      </div>\n    </div>\n    <!-- /Page Wrapper -->\n  </div>\n  <!-- /Main Wrapper -->\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      title: 'Rating',\n      items: [\n        {\n          text: 'Dashboard',\n          href: '/index',\n        },\n        {\n          text: 'Components',\n          active: true,\n        },\n      ],\n    };\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}