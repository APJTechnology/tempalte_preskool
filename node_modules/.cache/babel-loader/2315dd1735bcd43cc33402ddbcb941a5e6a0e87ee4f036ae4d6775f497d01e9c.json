n                            href=\"javascript:void(0);\"\r\n                            class=\"btn btn-sm bg-success-light me-2\"\r\n                          >\r\n                            <i class=\"fas fa-eye\"></i>\r\n                          </a>\r\n                          <router-link\r\n                            to=\"/edit-teacher\"\r\n                            class=\"btn btn-sm bg-danger-light\"\r\n                          >\r\n                            <i class=\"fas fa-edit\"></i>\r\n                          </router-link>\r\n                        </div>\r\n                      </template>\r\n                    </template>\r\n                  </a-table>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <layout-footer></layout-footer>\r\n    </div>\r\n    <!-- /Page Wrapper -->\r\n  </div>\r\n  <!-- /Main Wrapper -->\r\n</template>\r\n\r\n<script>\r\nimport { computed, reactive, toRefs } from \"vue\";\r\nconst columns = [\r\n  {\r\n    title: \"ID\",\r\n    dataIndex: \"teacher_id\",\r\n    sorter: {\r\n      compare: (a, b) => {\r\n        a = a.teacher_id.toLowerCase();\r\n        b = b.teacher_id.toLowerCase();\r\n        return a > b ? -1 : b > a ? 1 : 0;\r\n      },\r\n    },\r\n  },\r\n  {\r\n    title: \"Name\",\r\n    dataIndex: \"teacher_name\",\r\n    key: \"id\",\r\n    sorter: (a, b) => a.teacher_name.localeCompare(b.teacher_name),\r\n  },\r\n  {\r\n    title: \"Class\",\r\n    dataIndex: \"class\",\r\n    sorter: {\r\n      compare: (a, b) => {\r\n        a = a.class.toLowerCase();\r\n        b = b.class.toLowerCase();\r\n        return a > b ? -1 : b > a ? 1 : 0;\r\n      },\r\n    },\r\n  },\r\n  {\r\n    title: \"Gender\",\r\n    dataIndex: \"gender\",\r\n    sorter: (a, b) => a.gender.localeCompare(b.gender),\r\n  },\r\n  {\r\n    title: \"Subject\",\r\n    dataIndex: \"subject\",\r\n    sorter: {\r\n      compare: (a, b) => {\r\n        a = a.subject.toLowerCase();\r\n        b = b.subject.toLowerCase();\r\n        return a > b ? -1 : b > a ? 1 : 0;\r\n      },\r\n    },\r\n  },\r\n  {\r\n    title: \"Section\",\r\n    key: \"section\",\r\n    dataIndex: \"section\",\r\n    sorter: (a, b) => {\r\n      if (a.section === data[0].section || b.section === data[0].section) return 0;\r\n      return a.section.length - b.section.length;\r\n    },\r\n  },\r\n  {\r\n    title: \"Mobile Number\",\r\n    dataIndex: \"mobile_number\",\r\n    sorter: (a, b) => {\r\n      if (\r\n        a.mobile_number === data[0].mobile_number ||\r\n        b.mobile_number === data[0].mobile_number\r\n      )\r\n        return 0;\r\n      return a.mobile_number.length - b.mobile_number.length;\r\n    },\r\n  },\r\n  {\r\n    title: \"Address\",\r\n    dataIndex: \"address\",\r\n    sorter: (a, b) => {\r\n      if (a.address === data[0].address || b.address === data[0].address) return 0;\r\n      return a.address.length - b.address.length;\r\n    },\r\n  },\r\n  {\r\n    title: \"Action\",\r\n    key: \"action\",\r\n    class: \"text-end\",\r\n    sorter: true,\r\n  },\r\n];\r\nconst data = [\r\n  {\r\n    id: 1,\r\n    teacher_id: \"PRE1234\",\r\n    teacher_name: \"Nathan Humphries\",\r\n    image: \"avatar-11.jpg\",\r\n    class: \"10\",\r\n    gender: \"Male\",\r\n    subject: \"Biology\",\r\n    section: \"A\",\r\n    mobile_number: \"077 3499 9959\",\r\n    address: \"86 Lamphey Road, Thelnetham\",\r\n  },\r\n  {\r\n    id: 2,\r\n    teacher_id: \"PRE1434\",\r\n    teacher_name: \"Vincent\",\r\n    image: \"avatar-08.jpg\",\r\n    class: \"10\",\r\n    gender: \"Male\",\r\n    subject: \"Mathematics\",\r\n    section: \"C\",\r\n    mobile_number: \"402 221 7523\",\r\n    address: \"3979  Ashwood Drive, Omaha\",\r\n  },\r\n  {\r\n    id: 3,\r\n    teacher_id: \"PRE1534\",\r\n    teacher_name: \"Lois A\",\r\n    image: \"avatar-06.jpg\",\r\n    class: \"10\",\r\n    gender: \"Female\",\r\n    subject: \"English\",\r\