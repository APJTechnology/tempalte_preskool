{"ast":null,"code":"export default {\n  data: function data() {\n    return {\n      title: \"Sweet Alerts\",\n      items: [{\n        text: \"Dashboard\",\n        href: \"/index\"\n      }, {\n        text: \"Components\",\n        active: true\n      }]\n    };\n  },\n  methods: {\n    basicAlert: function basicAlert() {\n      this.$swal(\"Any fool can use a computer\");\n    },\n    withTitle: function withTitle() {\n      this.$swal({\n        title: \"The Internet?,\",\n        text: \"That thing is still around?\"\n      });\n    },\n    footerAlert: function footerAlert() {\n      this.$swal({\n        type: \"error\",\n        title: \"Oops...\",\n        text: \"Something went wrong!\",\n        footer: \"<a href>Why do I have this issue?</a>\"\n      });\n    },\n    positionTopstart: function positionTopstart() {\n      this.$swal({\n        position: \"top-start\",\n        type: \"success\",\n        title: \"Your work has been saved\",\n        timer: 1500,\n        showConfirmButton: !1\n      });\n    },\n    positionTopend: function positionTopend() {\n      this.$swal({\n        position: \"top-end\",\n        type: \"success\",\n        title: \"Your work has been saved\",\n        timer: 1500,\n        showConfirmButton: !1\n      });\n    },\n    positionBottomstart: function positionBottomstart() {\n      this.$swal({\n        position: \"bottom-start\",\n        type: \"success\",\n        title: \"Your work has been saved\",\n        timer: 1500,\n        showConfirmButton: !1\n      });\n    },\n    positionBottomend: function positionBottomend() {\n      this.$swal({\n        position: \"bottom-end\",\n        type: \"success\",\n        title: \"Your work has been saved\",\n        timer: 1500,\n        showConfirmButton: !1\n      });\n    },\n    typeSuccess: function typeSuccess() {\n      this.$swal({\n        title: \"Good job!\",\n        text: \"You clicked the button!\",\n        type: \"success\",\n        confirmButtonClass: \"btn btn-primary\",\n        buttonsStyling: !1\n      });\n    },\n    typeInfo: function typeInfo() {\n      this.$swal({\n        title: \"Info!\",\n        text: \"You clicked the button!\",\n        type: \"info\",\n        confirmButtonClass: \"btn btn-primary\",\n        buttonsStyling: !1\n      });\n    },\n    typeWarning: function typeWarning() {\n      this.$swal({\n        title: \"Warning!\",\n        text: \" You clicked the button!\",\n        type: \"warning\",\n        confirmButtonClass: \"btn btn-primary\",\n        buttonsStyling: !1\n      });\n    },\n    typeError: function typeError() {\n      this.$swal({\n        title: \"Error!\",\n        text: \" You clicked the button!\",\n        type: \"error\",\n        confirmButtonClass: \"btn btn-primary\",\n        buttonsStyling: !1\n      });\n    },\n    autoClose: function autoClose() {\n      this.$swal({\n        title: \"Auto close alert!\",\n        html: \"I will close in <strong></strong> seconds.\",\n        timer: 2e3,\n        confirmButtonClass: \"btn btn-primary\",\n        buttonsStyling: !1,\n        onBeforeOpen: function onBeforeOpen() {\n          swal.showLoading(), t = setInterval(function () {\n            swal.getContent().querySelector(\"strong\").textContent = Swal.getTimerLeft();\n          }, 100);\n        },\n        onClose: function onClose() {\n          clearInterval(t);\n        }\n      }).then(function (t) {\n        t.dismiss === swal.DismissReason.timer && console.log(\"I was closed by the timer\");\n      });\n    },\n    outsideClick: function outsideClick() {\n      this.$swal({\n        title: \"Click outside to close!\",\n        text: \"This is a cool message!\",\n        allowOutsideClick: !0,\n        confirmButtonClass: \"btn btn-primary\",\n        buttonsStyling: !1\n      });\n    },\n    promptFunction: function promptFunction() {\n      this.$swal({\n        input: \"text\",\n        confirmButtonText: \"Next &rarr;\",\n        showCancelButton: !0,\n        progressSteps: [\"1\", \"2\", \"3\"],\n        confirmButtonClass: \"btn btn-primary\",\n        buttonsStyling: !1,\n        cancelButtonClass: \"btn btn-danger ml-1\"\n      }).queue([{\n        title: \"Question 1\",\n        text: \"Chaining swal2 modals is easy\"\n      }, \"Question 2\", \"Question 3\"]).then(function (t) {\n        t.value && Swal.fire({\n          title: \"All done!\",\n          html: \"Your answers: <pre><code>\" + JSON.stringify(t.value) + \"</code></pre>\",\n          confirmButtonText: \"Lovely!\"\n        });\n      });\n    },\n    confirmText: function confirmText() {\n      this.$swal({\n        title: \"Are you sure?\",\n        text: \"You won't be able to revert this!\",\n        type: \"warning\",\n        showCancelButton: !0,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Yes, delete it!\",\n        confirmButtonClass: \"btn btn-primary\",\n        cancelButtonClass: \"btn btn-danger ml-1\",\n        buttonsStyling: !1\n      }).then(function (t) {\n        t.value && Swal.fire({\n          type: \"success\",\n          title: \"Deleted!\",\n          text: \"Your file has been deleted.\",\n          confirmButtonClass: \"btn btn-success\"\n        });\n      });\n    },\n    confirmColor: function confirmColor() {\n      this.$swal({\n        title: \"Are you sure?\",\n        text: \"You won't be able to revert this!\",\n        type: \"warning\",\n        showCancelButton: !0,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Yes, delete it!\",\n        confirmButtonClass: \"btn btn-primary\",\n        cancelButtonClass: \"btn btn-danger ml-1\",\n        buttonsStyling: !1\n      }).then(function (t) {\n        t.value ? Swal.fire({\n          type: \"success\",\n          title: \"Deleted!\",\n          text: \"Your file has been deleted.\",\n          confirmButtonClass: \"btn btn-success\"\n        }) : t.dismiss === Swal.DismissReason.cancel && Swal.fire({\n          title: \"Cancelled\",\n          text: \"Your imaginary file is safe :)\",\n          type: \"error\",\n          confirmButtonClass: \"btn btn-success\"\n        });\n      });\n    }\n  },\n  name: \"sweetalerts\"\n};","map":{"version":3,"mappings":"AAuJA,eAAe;EACbA,IAAI,kBAAG;IACL,OAAO;MACLC,KAAK,EAAE,cAAc;MACrBC,KAAK,EAAE,CACL;QACEC,IAAI,EAAE,WAAW;QACjBC,IAAI,EAAE;MACR,CAAC,EACD;QACED,IAAI,EAAE,YAAY;QAClBE,MAAM,EAAE;MACV,CAAC;IAEL,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAU,wBAAG;MACX,IAAI,CAACC,KAAK,CAAC,6BAA6B,CAAC;IAC3C,CAAC;IACDC,SAAS,uBAAG;MACV,IAAI,CAACD,KAAK,CAAC;QACTP,KAAK,EAAE,gBAAgB;QACvBE,IAAI,EAAE;MACR,CAAC,CAAC;IACJ,CAAC;IACDO,WAAW,yBAAG;MACZ,IAAI,CAACF,KAAK,CAAC;QACTG,IAAI,EAAE,OAAO;QACbV,KAAK,EAAE,SAAS;QAChBE,IAAI,EAAE,uBAAuB;QAC7BS,MAAM,EAAE;MACV,CAAC,CAAC;IACJ,CAAC;IACDC,gBAAgB,8BAAG;MACjB,IAAI,CAACL,KAAK,CAAC;QACTM,QAAQ,EAAE,WAAW;QACrBH,IAAI,EAAE,SAAS;QACfV,KAAK,EAAE,0BAA0B;QACjCc,KAAK,EAAE,IAAI;QACXC,iBAAiB,EAAE,CAAC;MACtB,CAAC,CAAC;IACJ,CAAC;IACDC,cAAc,4BAAG;MACf,IAAI,CAACT,KAAK,CAAC;QACTM,QAAQ,EAAE,SAAS;QACnBH,IAAI,EAAE,SAAS;QACfV,KAAK,EAAE,0BAA0B;QACjCc,KAAK,EAAE,IAAI;QACXC,iBAAiB,EAAE,CAAC;MACtB,CAAC,CAAC;IACJ,CAAC;IACDE,mBAAmB,iCAAG;MACpB,IAAI,CAACV,KAAK,CAAC;QACTM,QAAQ,EAAE,cAAc;QACxBH,IAAI,EAAE,SAAS;QACfV,KAAK,EAAE,0BAA0B;QACjCc,KAAK,EAAE,IAAI;QACXC,iBAAiB,EAAE,CAAC;MACtB,CAAC,CAAC;IACJ,CAAC;IACDG,iBAAiB,+BAAG;MAClB,IAAI,CAACX,KAAK,CAAC;QACTM,QAAQ,EAAE,YAAY;QACtBH,IAAI,EAAE,SAAS;QACfV,KAAK,EAAE,0BAA0B;QACjCc,KAAK,EAAE,IAAI;QACXC,iBAAiB,EAAE,CAAC;MACtB,CAAC,CAAC;IACJ,CAAC;IACDI,WAAW,yBAAG;MACZ,IAAI,CAACZ,KAAK,CAAC;QACTP,KAAK,EAAE,WAAW;QAClBE,IAAI,EAAE,yBAAyB;QAC/BQ,IAAI,EAAE,SAAS;QACfU,kBAAkB,EAAE,iBAAiB;QACrCC,cAAc,EAAE,CAAC;MACnB,CAAC,CAAC;IACJ,CAAC;IACDC,QAAQ,sBAAG;MACT,IAAI,CAACf,KAAK,CAAC;QACTP,KAAK,EAAE,OAAO;QACdE,IAAI,EAAE,yBAAyB;QAC/BQ,IAAI,EAAE,MAAM;QACZU,kBAAkB,EAAE,iBAAiB;QACrCC,cAAc,EAAE,CAAC;MACnB,CAAC,CAAC;IACJ,CAAC;IACDE,WAAW,yBAAG;MACZ,IAA