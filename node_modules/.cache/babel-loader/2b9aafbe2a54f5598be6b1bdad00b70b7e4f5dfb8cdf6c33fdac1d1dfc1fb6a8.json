n                <div class=\"card-title\">Confirm</div>\n              </div>\n              <div class=\"card-body\">\n                <a\n                  @click=\"confirm\"\n                  href=\"javascript: void(0);\"\n                  id=\"alert-confirm\"\n                  class=\"btn btn-primary waves-effect waves-light\"\n                  >Click me</a\n                >\n              </div>\n            </div>\n          </div>\n          <!-- /Alert -->\n\n          <!-- Alert -->\n          <div class=\"col-md-4\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <div class=\"card-title\">Prompt</div>\n              </div>\n              <div class=\"card-body\">\n                <a\n                  @click=\"prompt\"\n                  href=\"javascript: void(0);\"\n                  id=\"alert-prompt\"\n                  class=\"btn btn-primary waves-effect waves-light\"\n                  >Click me</a\n                >\n              </div>\n            </div>\n          </div>\n          <!-- /Alert -->\n\n          <!-- Alert -->\n          <div class=\"col-md-4\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <div class=\"card-title\">Success Alert</div>\n              </div>\n              <div class=\"card-body\">\n                <a\n                  @click=\"openNotification()\"\n                  href=\"javascript: void(0);\"\n                  id=\"alert-success\"\n                  class=\"btn btn-primary btn-sm waves-effect waves-light\"\n                  >Click me</a\n                >\n              </div>\n            </div>\n          </div>\n          <!-- /Alert -->\n\n          <!-- Alert -->\n          <div class=\"col-md-4\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <div class=\"card-title\">Error Alert</div>\n              </div>\n              <div class=\"card-body\">\n                <a\n                  @click=\"openNotificationerr()\"\n                  href=\"javascript: void(0);\"\n                  id=\"alert-error\"\n                  class=\"btn btn-primary btn-sm waves-effect waves-light\"\n                  >Click me</a\n                >\n              </div>\n            </div>\n          </div>\n          <!-- /Alert -->\n\n          <!-- Alert -->\n          <div class=\"col-md-4\">\n            <div class=\"card\">\n              <div class=\"card-header\">\n                <div class=\"card-title\">Warnng Alert</div>\n              </div>\n              <div class=\"card-body\">\n                <a\n                  @click=\"openNotificationwarr()\"\n                  href=\"javascript: void(0);\"\n                  id=\"alert-warning\"\n                  class=\"btn btn-primary btn-sm waves-effect waves-light\"\n                  >Click me</a\n                >\n              </div>\n            </div>\n          </div>\n          <!-- /Alert -->\n        </div>\n      </div>\n      <layout-footer></layout-footer>\n    </div>\n    <!-- /Page Wrapper -->\n  </div>\n  <!-- /Main Wrapper -->\n</template>\n<script>\nimport { notification } from 'ant-design-vue';\nexport default {\n  data() {\n    return {\n      title: 'Notification',\n      items: [\n        {\n          text: 'Dashboard',\n          href: '/index',\n        },\n        {\n          text: 'Components',\n          active: true,\n        },\n      ],\n    };\n  },\n  setup() {\n    const openNotification = () => {\n      notification.open({\n        message: 'Success message',\n        placement: 'bottomRight',\n        bottom: '50px',\n        duration: 3,\n        style: {\n          width: '250px',\n          marginLeft: `${335 - 600}px`,\n          background: '#2ab57d',\n        },\n      });\n    };\n    const openNotificationerr = () => {\n      notification.open({\n        message: 'Error message',\n        placement: 'bottomRight',\n        bottom: '50px',\n        duration: 3,\n        style: {\n          width: '250px',\n          marginLeft: `${335 - 600}px`,\n          background: '#fd625e',\n        },\n      });\n    };\n    const openNotificationwarr = () => {\n      notification.open({\n        message: 'Warning message',\n        placement: 'bottomRight',\n        bottom: '50px',\n        duration: 3,\n        style: {\n          width: '250px',\n          marginLeft: `${335 - 600}px`,\n          background: '#ffbf53',\n        },\n      });\n    };\n    return {\n      openNotification,\n      openNotificationerr,\n      openNotificationwarr,\n    };\n  },\n  methods: {\n    success() {\n      this.$swal({\n        position: 'top',\n        title: 'Alert Title',\n        text: 'Alert Message',\n      });\n    },\n\n    confirm() {\n      this.$swal({\n        position: 'top',\n        title: 'AlertifyJS',\n        text: 'This is a confirm dialog.',\n        confirmButtonColor: '#2fdf84',\n        showCancelButton: !0,\n        cancelButtonColor: '#000',\n        confirmButtonText: 'ok',\n        cancelButtonClass: 'btn btn-danger ml-1',\n      });\n    },\n    prompt() {\n      this.$swal({\n        position: 'top',\n        input: 'text',\n        confirmButtonText: 'ok',\n        showCancelButton: !0,\n        confirmButtonClass: 'btn btn-primary',\n        buttonsStyling: !1,\n        cancelButtonClass: 'btn btn-danger ml-1',\n      });\n    },\n  },\n};\n</script>\n<style>\n.ant-notification-notice-closable .ant-notification-notice-message {\n  color: white;\n}\n.ant-notification-close-x {\n  display: none;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}