/div>\n                  <div class=\"form-group\">\n                    <label>Date</label>\n                    <b-form-input\n                      type=\"text\"\n                      id=\"date\"\n                      class=\"form-control\"\n                    ></b-form-input>\n                    <span class=\"form-text text-muted\">dd/mm/yyyy</span>\n                  </div>\n                  <div class=\"form-group\">\n                    <label>SSN field 1</label>\n                    <b-form-input\n                      type=\"text\"\n                      id=\"ssn\"\n                      class=\"form-control\"\n                    ></b-form-input>\n                    <span class=\"form-text text-muted\">e.g \"999-99-9999\"</span>\n                  </div>\n                  <div class=\"form-group\">\n                    <label>Phone field + ext.</label>\n                    <b-form-input\n                      type=\"text\"\n                      id=\"phoneExt\"\n                      class=\"form-control\"\n                    ></b-form-input>\n                    <span class=\"form-text text-muted\">+40 999 999 999</span>\n                  </div>\n                  <div class=\"form-group\">\n                    <label>Product Key</label>\n                    <b-form-input\n                      type=\"text\"\n                      id=\"product\"\n                      class=\"form-control\"\n                    ></b-form-input>\n                    <span class=\"form-text text-muted\">e.g a*-999-a999</span>\n                  </div>\n                  <div class=\"form-group\">\n                    <label>Currency</label>\n                    <b-form-input\n                      type=\"text\"\n                      id=\"currency\"\n                      class=\"form-control\"\n                    ></b-form-input>\n                    <span class=\"form-text text-muted\">$ 999,999,999.99</span>\n                  </div>\n                  <div class=\"form-group\">\n                    <label>Eye Script</label>\n                    <b-form-input\n                      type=\"text\"\n                      id=\"eyescript\"\n                      class=\"form-control\"\n                    ></b-form-input>\n                    <span class=\"form-text text-muted\">~9.99 ~9.99 999</span>\n                  </div>\n                  <div class=\"form-group\">\n                    <label>Percent</label>\n                    <b-form-input\n                      type=\"text\"\n                      id=\"pct\"\n                      class=\"form-control\"\n                    ></b-form-input>\n                    <span class=\"form-text text-muted\">e.g \"99%\"</span>\n                  </div>\n                  <div class=\"form-group mb-0\">\n                    <label>Credit Card Number</label>\n                    <b-form-input\n                      type=\"text\"\n                      class=\"form-control\"\n                      id=\"ccn\"\n                    ></b-form-input>\n                    <span class=\"form-text text-muted\"\n                      >e.g \"999.999.999.9999\"</span\n                    >\n                  </div>\n                </b-form>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <layout-footer></layout-footer>\n    </div>\n    <!-- /Page Wrapper -->\n  </div>\n  <!-- /Main Wrapper -->\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      title: 'Form Mask',\n      items: [\n        {\n          text: 'Dashboard',\n          href: '/index',\n        },\n        {\n          text: 'Form Mask',\n          active: true,\n        },\n      ],\n    };\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}