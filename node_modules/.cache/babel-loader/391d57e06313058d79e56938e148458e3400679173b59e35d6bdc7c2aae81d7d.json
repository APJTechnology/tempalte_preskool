{"ast":null,"code":"export default {\n  data: function data() {\n    return {\n      title: 'Spinner',\n      items: [{\n        text: 'Dashboard',\n        href: '/index'\n      }, {\n        text: 'Components',\n        active: true\n      }],\n      variants: ['primary', 'secondary', 'success', 'danger', 'warning', 'info', 'light', 'dark', 'blue', 'pink']\n    };\n  }\n};","map":{"version":3,"mappings":"AAyQA,eAAe;EACbA,IAAI,kBAAG;IACL,OAAO;MACLC,KAAK,EAAE,SAAS;MAChBC,KAAK,EAAE,CACL;QACEC,IAAI,EAAE,WAAW;QACjBC,IAAI,EAAE;MACR,CAAC,EACD;QACED,IAAI,EAAE,YAAY;QAClBE,MAAM,EAAE;MACV,CAAC,CACF;MACDC,QAAQ,EAAE,CACR,SAAS,EACT,WAAW,EACX,SAAS,EACT,QAAQ,EACR,SAAS,EACT,MAAM,EACN,OAAO,EACP,MAAM,EACN,MAAM,EACN,MAAM;IAEV,CAAC;EACH;AACF,CAAC","names":["data","title","items","text","href","active","variants"],"sourceRoot":"","sources":["/home/jesus-adson/projects/erp-front/src/views/pages/uiinterface/baseui/spinners.vue"],"sourcesContent":["<template>\n  <!-- Main Wrapper -->\n  <div class=\"main-wrapper\">\n    <layout-header></layout-header>\n    <layout-sidebar></layout-sidebar>\n\n    <!-- Page Wrapper -->\n    <div class=\"page-wrapper\">\n      <div class=\"content container-fluid\">\n        <componentheader :title=\"title\" :items=\"items\" />\n        <div class=\"row\">\n          <div class=\"col-lg-6\">\n            <div class=\"card\">\n              <div class=\"card-body\">\n                <h4 class=\"mt-0 header-title\">Border spinner</h4>\n                <p class=\"mb-4\">\n                  Use the border spinners for a lightweight loading indicator.\n                </p>\n                <b-spinner label=\"Loading...\" class=\"m-2\"></b-spinner>\n              </div>\n              <!--end card-body-->\n            </div>\n            <!--end card-->\n          </div>\n          <!-- end col -->\n\n          <div class=\"col-lg-6\">\n            <div class=\"card\">\n              <div class=\"card-body\">\n                <h4 class=\"mt-0 header-title\">Growing spinner</h4>\n                <p class=\"mb-4\">\n                  If you don’t fancy a border spinner, switch to the grow\n                  spinner. While it doesn’t technically spin, it does repeatedly\n                  grow!\n                </p>\n                <b-spinner type=\"grow\" label=\"Loading...\"></b-spinner>\n              </div>\n              <!--end card-body-->\n            </div>\n            <!--end card-->\n          </div>\n          <!-- end col -->\n        </div>\n        <!-- end row -->\n\n        <div class=\"row\">\n          <div class=\"col-lg-6\">\n            <div class=\"card\">\n              <div class=\"card-body\">\n                <h4 class=\"mt-0 header-title\">Colors</h4>\n                <p class=\"mb-4\">\n                  You can use any of our text color utilities on the standard\n                  spinner.\n                </p>\n                <b-spinner\n                  v-for=\"variant in variants\"\n                  :variant=\"variant\"\n                  :key=\"variant\"\n                  class=\"m-2\"\n                ></b-spinner>\n              </div>\n              <!--end card-body-->\n            </div>\n            <!--end card-->\n          </div>\n          <!-- end col -->\n\n          <div class=\"col-lg-6\">\n            <div class=\"card\">\n              <div class=\"card-body\">\n                <h4 class=\"mt-0 header-title\">Color Growing spinner</h4>\n                <p class=\"mb-4\">\n                  Here it is in blue, along with the supported variants.\n                </p>\n                <b-spinner\n                  v-for=\"variant in variants\"\n                  :variant=\"variant\"\n                  :key=\"variant\"\n                  type=\"grow\"\n                  class=\"m-2\"\n                ></b-spinner>\n              </div>\n              <!--end card-body-->\n            </div>\n            <!--end card-->\n          </div>\n          <!-- end col -->\n        </div>\n        <!-- end row -->\n\n        <div class=\"row\">\n          <div class=\"col-lg-6\">\n            <div class=\"card\">\n              <div class=\"card-body\">\n                <h4 class=\"mt-0 header-title\">Alignment</h4>\n                <p class=\"mb-4\">\n                  Use flexbox utilities, float utilities, or text alignment\n                  utilities to place spinners exactly where you need them in any\n                  situation.\n                </p>\n                <div class=\"d-flex justify-content-center\">\n                  <b-spinner label=\"Loading...\"></b-spinner>\n                </div>\n              </div>\n              <!--end card-body-->\n            </div>\n            <!--end card-->\n          </div>\n          <!-- end col -->\n\n          <div class=\"col-lg-6\">\n            <div class=\"card\">\n              <div class=\"card-body\">\n                <h4 class=\"mt-0 header-title\">Placement</h4>\n                <p class=\"mb-4\">\n                  Use <code>flexbox utilities</code>,\n                  <code>float utilities</code>, or\n                  <code>text alignment</code> utilities to place spinners\n                  exactly where you need them in any situation.\n                </p>\n                <div class=\"d-flex align-items-center\">\n                  <strong>Loading...</strong>\n                  <b-spinner label=\"Loading...\" class=\"ms-auto\"></b-spinner>\n                </div>\n              </div>\n              <!--end card-body-->\n            </div>\n            <!--end card-->\n          </div>\n          <!-- end col -->\n        </div>\n        <!-- end row -->\n\n        <div class=\"row\">\n          <div class=\"col-lg-6\">\n            <div class=\"card\">\n              <div class=\"card-body\">\n                <h4 class=\"mt-0 header-title\">Size</h4>\n                <p class=\"mb-4\">\n                  Add <code>.spinner-border-sm</code> and\n                  <code>.spinner-border.avatar-**</code>\n                  to make a smaller spinner that can quickly be used within\n                  other components.\n                </p>\n                <div class=\"row\">\n                  <div class=\"col-lg-6\">\n                    <b-spinner\n                      label=\"Large Spinner\"\n                      class=\"avatar-lg text-primary m-2\"\n                    ></b-spinner>\n                    <b-spinner\n                      type=\"grow\"\n                      label=\"Large Spinner\"\n                      class=\"avatar-lg text-secondary m-2\"\n                    ></b-spinner>\n                  </div>\n                  <!-- end col -->\n\n                  <div class=\"col-lg-6\">\n                    <b-spinner\n                      label=\"Large Spinner\"\n                      class=\"avatar-md text-primary m-2\"\n                    ></b-spinner>\n                    <b-spinner\n                      type=\"grow\"\n                      label=\"Large Spinner\"\n                      class=\"avatar-md text-secondary m-2\"\n                    ></b-spinner>\n                  </div>\n                  <!-- end col -->\n\n                  <div class=\"col-lg-6\">\n                    <b-spinner\n                      label=\"Large Spinner\"\n                      class=\"avatar-sm text-primary m-2\"\n                    ></b-spinner>\n                    <b-spinner\n                      type=\"grow\"\n                      label=\"Large Spinner\"\n                      class=\"avatar-sm text-secondary m-2\"\n                    ></b-spinner>\n                  </div>\n                  <!-- end col -->\n\n                  <div class=\"col-lg-6\">\n                    <b-spinner\n                      small\n                      label=\"Small Spinner\"\n                      class=\"m-2\"\n                    ></b-spinner>\n                    <b-spinner\n                      small\n                      label=\"Small Spinner\"\n                      type=\"grow\"\n                      class=\"m-2\"\n                    ></b-spinner>\n                  </div>\n                  <!-- end col -->\n                </div>\n                <!--end row-->\n              </div>\n              <!--end card-body-->\n            </div>\n            <!--end card-->\n          </div>\n          <!-- end col -->\n\n          <div class=\"col-lg-6\">\n            <div class=\"card\">\n              <div class=\"card-body\">\n                <h4 class=\"mt-0 header-title\">Buttons spinner</h4>\n                <p class=\"mb-4\">\n                  Use spinners within buttons to indicate an action is currently\n                  processing or taking place. You may also swap the text out of\n                  the spinner element and utilize button text as needed.\n                </p>\n                <div class=\"row\">\n                  <div class=\"col-md-6\">\n                    <div class=\"button-list mb-1 mb-sm-0\">\n                      <b-button variant=\"primary me-1\" disabled>\n                        <b-spinner class=\"spinner-border-sm\"></b-spinner>\n                      </b-button>\n                      <b-button variant=\"primary\" disabled>\n                        <b-spinner class=\"spinner-border-sm me-1\"></b-spinner>\n                        Loading...\n                      </b-button>\n                    </div>\n                  </div>\n                  <!-- end col -->\n                  <div class=\"col-md-6\">\n                    <div class=\"button-list\">\n                      <b-button variant=\"primary me-1\" disabled>\n                        <b-spinner\n                          type=\"grow\"\n                          class=\"spinner-grow-sm\"\n                        ></b-spinner>\n                      </b-button>\n                      <b-button variant=\"primary\" disabled>\n                        <b-spinner\n                          type=\"grow\"\n                          class=\"spinner-grow-sm me-1\"\n                        ></b-spinner>\n                        Loading...\n                      </b-button>\n                    </div>\n                  </div>\n                  <!-- end col -->\n                </div>\n                <!-- end row -->\n              </div>\n              <!--end card-body-->\n            </div>\n            <!--end card-->\n          </div>\n          <!-- end col -->\n        </div>\n        <!-- end row -->\n      </div>\n      <layout-footer></layout-footer>\n    </div>\n    <!-- /Page Wrapper -->\n  </div>\n  <!-- /Main Wrapper -->\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      title: 'Spinner',\n      items: [\n        {\n          text: 'Dashboard',\n          href: '/index',\n        },\n        {\n          text: 'Components',\n          active: true,\n        },\n      ],\n      variants: [\n        'primary',\n        'secondary',\n        'success',\n        'danger',\n        'warning',\n        'info',\n        'light',\n        'dark',\n        'blue',\n        'pink',\n      ],\n    };\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}